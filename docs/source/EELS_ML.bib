doi = {https://doi.org/10.1002/pssb.2220960116},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pssb.2220960116},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/pssb.2220960116},
abstract = {Abstract The behaviour of the infrared-active lattice vibrations of GaS, GaSe, InSe are studied by measuring the room-temperature reflectivity in the 100 to 500 cmâ1 frequency range when the incident wave vector is inclined at an angle Îž to the crystallographic Z-axis. The experiments are performed in polarized light. The transverse and longitudinal mode frequencies are obtained by Kramers-Kronig analysis of the reststrahlen bands observed in these crystals with Îž between Îž and 90Â°. The experimentally observed changes of the lattice frequencies with an angle Îž are interpreted on the basis of the theory of long-wavelength lattice vibrations in anisotropic crystals. Good agreement between the experimentally observed dependence and the calculated one according to the theory gives the possibility to elucidate the nature of a fine structure observed in the reststrahlen region of the layered GaS, GaSe, InSe crystals.},
year = {1979}
}

