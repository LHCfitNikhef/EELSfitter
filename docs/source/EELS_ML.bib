@article{https://doi.org/10.1002/pssb.2220960116,
author = {Allakhverdiev, K. R. and Babaev, S. S. and Salaev, E. Yu. and Tagyev, M. M.},
title = {Angular behaviour of the polar optical phonons in AIIIBVI layered semiconductors},
journal = {physica status solidi (b)},
